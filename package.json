    "framer-motion": "^11.0.25",
    "jose": "^5.2.4",
    "lucide-react": "^0.363.0",
    "next": "14.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
